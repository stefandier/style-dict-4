    "data-simple/tokens/00 - Primitives/Default.json",
    "data-simple/tokens/01 - Semantic Colors/*.json"
  ],
  "preprocessors": [
    "tokens-studio"
  ],
  "platforms": {
    "react-native": {
      "buildPath": "build/",
      "prefix": "",
      "files": [
        {
          "destination": "mod.d.ts",
